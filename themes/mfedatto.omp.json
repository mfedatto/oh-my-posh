{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#EAF6FC",
          "foreground": "#1A2A33",
          "leading_diamond": "",
          "style": "diamond",
          "template": "\u2770 {{ if .Root }}\u272f {{ end }}{{ .UserName }} @ {{ if .WSL }}WSL at {{ end }}{{ .HostName }} \u2771",
          "trailing_diamond": "",
          "type": "os"
        },
        {
          "background": "#00FFFF",
          "foreground": "#1A2A33",
          "leading_diamond": "",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": "\u2770 {{ .CurrentDate | date .Format }} \u2771",
          "trailing_diamond": "",
          "type": "time"
        },
        {
          "background": "#20FFA6",
          "foreground": "#1A2A33",
          "leading_diamond": "",
          "style": "diamond",
          "template": "\u2770 CPU: {{ round .PhysicalPercentUsed .Precision }}% | ",
          "type": "sysinfo"
        },
        {
          "background": "#20FFA6",
          "foreground": "#1A2A33",
          "style": "diamond",
          "template": "RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1068786176.0) }}GB  \u2771",
          "trailing_diamond": "",
          "type": "sysinfo"
        },
        {
          "background": "transparent",
          "foreground": "#004d66",
          "leading_diamond": "",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#EAF6FC",
          "foreground": "#1A2A33",
          "leading_diamond": "",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#20FFA6>npm</> ",
            "yarn_icon": " <#00FFFF>yarn</>"
          },
          "style": "diamond",
          "template": "\u2770 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \u2771",
          "trailing_diamond": "",
          "type": "node"
        },
        {
          "background": "#FFE500",
          "foreground": "#1A2A33",
          "leading_diamond": "",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": "\u2770 {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \u270e {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \u2611 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \u26f6 {{ .StashCount }}{{ end }} \u2771",
          "trailing_diamond": "",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#EAF6FC",
          "properties": {
            "folder_icon": "\u2ba1",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "style": "diamond",
          "template": "\u2ba1 {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#20FFA6",
          "foreground_templates": ["{{ if gt .Code 0 }}#1A2A33{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u03bb ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#EAF6FC",
    "template": "\u03bb "
  },
  "version": 2
}
