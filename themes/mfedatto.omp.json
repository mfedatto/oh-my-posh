{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#EAF6FC",
          "foreground": "#1A2A33",
          "leading_diamond": "\ue0b2",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a "
          },
          "style": "diamond",
          "template": " {{ if .Root }}\uea86 {{ end }}{{ .UserName }} @ {{ if .WSL }}WSL at {{ end }}{{ .HostName }} {{ .Icon }}",
          "trailing_diamond": "<transparent,#EAF6FC>\ue0b2</>",
          "type": "os"
        },
        {
          "background": "#00FFFF",
          "foreground": "#1A2A33",
          "leading_diamond": "\ue0b2",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": " \ue384 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "<transparent,#00FFFF>\ue0b2</>",
          "type": "time"
        },
        {
          "background": "#20FFA6",
          "foreground": "#1A2A33",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue266 CPU: {{ round .PhysicalPercentUsed .Precision }}% | ",
          "type": "sysinfo"
        },
        {
          "background": "#20FFA6",
          "foreground": "#1A2A33",
          "style": "diamond",
          "template": "RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1068786176.0) }}GB \ue266 ",
          "trailing_diamond": "<transparent,#20FFA6>\ue0b2</>",
          "type": "sysinfo"
        },
        {
          "background": "#00FFFF",
          "foreground": "#1A2A33",
          "leading_diamond": "\ue0b2",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#EAF6FC",
          "foreground": "#1A2A33",
          "leading_diamond": "\ue0b2",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#20FFA6>\ue5fa</> ",
            "yarn_icon": " <#00FFFF>\ue6a7</>"
          },
          "style": "diamond",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": "<transparent,#EAF6FC>\ue0b2</>",
          "type": "node"
        },
        {
          "background": "#FFE500",
          "foreground": "#1A2A33",
          "leading_diamond": "\ue0b2",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#EAF6FC",
          "properties": {
            "folder_icon": "\uf07b",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#20FFA6",
          "foreground_templates": ["{{ if gt .Code 0 }}#1A2A33{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \uf054",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#EAF6FC",
    "template": "\ue285 "
  },
  "version": 2
}
